/**index.wxss**/
.stepBar{
  width: 100%;
}
/*ss 为实心对勾*/
.ss wux-icon view
{
  color:#fff;
  background-color: #387ef5;
  border:none;
  border-radius:100%;

}
.stepitem view.wux-step__title{
  font-size: 28rpx;
  font-weight: 100;
  color: #444
}
.upimg{
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  box-sizing: border-box;
  margin: 30rpx 0;
  padding: 0 20rpx;
}
.upimgItem{
  width: 48%;
}
.upimgItem image{
  width: 100%;
  height: 100%;
  margin-top: 20rpx;
}
.form{
  width: 100%;
  box-sizing: border-box;
  padding:0 10rpx 0 0
}
.timepick{
  opacity: 1 !important;
}
.jingshibox{
  background-color: #FFFAEE;
  display: flex;
  align-items: center;
  color: #FF7D3E;
  padding: 16rpx 20rpx;
  box-sizing: border-box;
  width: 100%;
  margin-top: 30rpx;
}
.jingshiicon{
  width: 36rpx
}