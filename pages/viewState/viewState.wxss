
.title{
  width: 100%;
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  box-sizing: border-box;
  margin: 31rpx 0;
}
.linelable{
  width: 8rpx;
  height: 32rpx;
  background-color: #FF8F00;
  margin-right: 7rpx;
}
.title text{
  font-size: 32rpx;
  color: #333;
  font-weight: bold;
}
.upimg{
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  box-sizing: border-box;
  margin: 30rpx 0;
  padding: 0 20rpx;
}
.upimgItem{
  width: 48%;
}
.upimgItem image{
  width: 100%;
  height: 100%;
  margin-top: 20rpx;
}
.form{
  width: 100%;
  box-sizing: border-box;
  padding:0 10rpx 0 0
}
.timepick{
  opacity: 1 !important;
}
.images{
  width: 48%
}